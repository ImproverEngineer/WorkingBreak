<?xml version="1.0" encoding="utf-8"?>
<!--			Конфигурация программы 
		TimeWork               - Время отслеживания активности в милисекундах
	    TimeWithoutWorkВремя   - отслеживания бездействия в милисекундах
		
						Настройки Message Window
		DecayInterval          - Интервал затухания в милисекундах
		widtWindowDeflection   - Отклонение по горизонтали в px
		heightWindowDeflection - Отклонение по вертикале в px
		Opacity 			   - Прозрачность ставится в % 
		textHeder			   - Строка заголовок 
		textBodyFirs		   - Текст 1
		textBodySec			   - Текст 2
		textFooter			   - Последня строка	-->
<configuration>
		<appSettings>		
			<add key="TimeWork" value="10000"/>		
			<add key="TimeWithoutWork" value="10000"/>	
			<add key="DecayInterval" value="1000"/>		
			<add key="widtWindowDeflection" value="5"/>	
			<add key="heightWindowDeflection" value="50"/>
			<add key="Opacity" value ="60"/>
			<add key="textHeder" value="Отдохни, если устал."/>
			<add key="textBodyFirs" value="Сделай зарядку."/>
			<add key="textBodySec" value="Подумай о море."/>
			<add key="textFooter" value ="Выпей кофе"/>			
		</appSettings>   
</configuration>